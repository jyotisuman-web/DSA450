class Node:
    def __init__(self,data):
        self.data=data
        self.next=None
#travesal of Linked List
def traversal(head):
    currentnode=head
    while currentnode:
        print(currentnode.data,end="->")
        currentnode=currentnode.next
   print("null")
node1=Node(5)
node2=Node(4)
node3=Node(3)
node4=Node(2)
node5=Node(1)
node1.next=node2
node2.next=node3
node3.next=node4
node4.next=node5
traversal(node1)
